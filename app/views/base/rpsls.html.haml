#main_left
	-@strategies.each_with_index do |h, i|
		= link_to '', rpsls_path(:player_move => h), :name => "strategy_#{i}"
	= image_tag 'rules.png', :alt => 'rules'
#main_center
	-if flash[:leaderboard]
		= render 'leaderboard'
	-else
		= render :partial => 'game'

#main_right
	= render :partial => 'leaderboard_summ' if !flash[:leaderboard]

	
	